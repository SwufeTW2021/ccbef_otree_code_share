list1 = [1, 4, 6, 7, 8, 9, 11, 13, 15, 16, 19, 20, 22, 23, 25, 32, 34, 35, 39, 40]
list2 = [2, 3, 5, 10, 12, 14, 17, 18, 21, 24, 26, 27, 28, 29, 30, 31, 33, 36, 37, 38]
for p in self.get_players():
    if list1:
        a = list1[0]
        p.number = a
        list1 = list1.remove(a)
    else:
        a = list2[0]
        p.number = a
        list2 = list2.remove(a)
    
